	"homepage": "https://pack11ty.dev/",
	"author": {
		"name": "Nicolas Hoizey",
		"url": "https://nicolas-hoizey.com/",
		"email": "nicolas@hoizey.com"
	},
	"keywords": [
		"eleventy",
		"starter",
		"template",
		"eleventy-website",
		"eleventy-sample"
	],
	"repository": {
